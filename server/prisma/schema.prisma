generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  name String 
  email String @unique
  nickName String @unique
  role String
  phone String
  password String 
  createdAt DateTime @default(now())
}
